{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/include",
                "${workspaceFolder}/tests",
                "${workspaceFolder}/lib",
                "${workspaceFolder}/lib/glm",
                "${workspaceFolder}/lib/lvgl",
                "${workspaceFolder}/lib/lvgl/src",
                "${workspaceFolder}/lib/libhv/include",
                "${workspaceFolder}/lib/libhv/cpputil",
                "${workspaceFolder}/lib/libhv",
                "${workspaceFolder}/lib/spdlog/include",
                "${workspaceFolder}/lib/tinygl/include",
                "${workspaceFolder}/lib/tinygl/include-demo",
                "${workspaceFolder}/lib/wpa_supplicant/src/common",
                "${workspaceFolder}/lib/wpa_supplicant/src/utils",
                "${workspaceFolder}/lib/sdl2/include",
                "${workspaceFolder}/lib/sdl2/include/SDL2",
                "/opt/homebrew/include",
                "/opt/homebrew/include/SDL2"
            ],
            "defines": [
                "LV_CONF_INCLUDE_SIMPLE",
                "LV_LVGL_H_INCLUDE_SIMPLE",
                "HELIX_DISPLAY_SDL",
                "ENABLE_TINYGL_3D"
            ],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-arm64",
            "compileCommands": "${workspaceFolder}/compile_commands.json"
        }
    ],
    "version": 4
}

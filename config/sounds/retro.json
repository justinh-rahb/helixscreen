    "print_complete": {
      "description": "Triumphant chiptune victory fanfare — Mario-style ascending melody",
      "bpm": 170,
      "steps": [
        { "note": "C5", "dur": "8n", "vel": 0.9, "duty": 0.5,
          "env": { "a": 2, "d": 15, "s": 0.6, "r": 10 } },
        { "note": "E5", "dur": "8n", "vel": 0.9, "duty": 0.5,
          "env": { "a": 2, "d": 15, "s": 0.6, "r": 10 } },

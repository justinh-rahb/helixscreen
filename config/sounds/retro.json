    "error_alert": {
      "description": "Harsh buzzy 8-bit alarm with rapid tremolo",
      "steps": [
        { "freq": 880, "dur": 100, "wave": "square", "vel": 1.0, "duty": 0.15,
          "lfo": { "target": "amplitude", "rate": 20, "depth": 0.6 },
          "env": { "a": 1, "d": 10, "s": 0.9, "r": 10 } },
        { "pause": 50 },
        { "freq": 660, "dur": 100, "wave": "square", "vel": 1.0, "duty": 0.15,
          "lfo": { "target": "amplitude", "rate": 20, "depth": 0.6 },
          "env": { "a": 1, "d": 10, "s": 0.9, "r": 10 } },
        { "pause": 50 },
        { "freq": 880, "dur": 100, "wave": "square", "vel": 1.0, "duty": 0.15,
          "lfo": { "target": "amplitude", "rate": 20, "depth": 0.6 },
          "env": { "a": 1, "d": 10, "s": 0.9, "r": 10 } }
      ],
      "repeat": 2
    },
    "error_tone": {
      "description": "Short buzzy error beep — wrong answer sound",
      "steps": [
        { "note": "E4", "dur": 60, "vel": 0.85, "duty": 0.15,
          "env": { "a": 1, "d": 15, "s": 0.5, "r": 10 } },
        { "pause": 30 },
        { "note": "C4", "dur": 90, "vel": 0.75, "duty": 0.15,
          "env": { "a": 1, "d": 20, "s": 0.3, "r": 15 } }
      ]
    },
    "alarm_urgent": {
      "description": "Aggressive saw alarm with rapid pitch wobble",
      "steps": [
        { "freq": 1760, "dur": 150, "wave": "saw", "vel": 1.0,
          "lfo": { "target": "freq", "rate": 12, "depth": 400 },
          "env": { "a": 1, "d": 10, "s": 0.95, "r": 10 } },
        { "pause": 60 },
        { "freq": 1320, "dur": 150, "wave": "saw", "vel": 1.0,
          "lfo": { "target": "freq", "rate": 12, "depth": 400 },
          "env": { "a": 1, "d": 10, "s": 0.95, "r": 10 } },
        { "pause": 60 }
      ],
      "repeat": 5
    },
    "test_beep": {
      "description": "Classic 8-bit test tone arpeggio",
      "bpm": 170,
      "steps": [
        { "note": "C5", "dur": "16n", "vel": 0.9, "duty": 0.5,
          "env": { "a": 1, "d": 10, "s": 0.4, "r": 8 } },
        { "note": "E5", "dur": "16n", "vel": 0.9, "duty": 0.5,
          "env": { "a": 1, "d": 10, "s": 0.4, "r": 8 } },
        { "note": "G5", "dur": "16n", "vel": 0.95, "duty": 0.5,
          "env": { "a": 1, "d": 10, "s": 0.4, "r": 8 } },
        { "note": "C6", "dur": "8n", "vel": 1.0, "duty": 0.5,
          "env": { "a": 2, "d": 20, "s": 0.5, "r": 15 } }
      ]
    }
  }
}

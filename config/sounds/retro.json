{
  "name": "retro",
  "description": "8-bit chiptune theme — square waves, fast arpeggios, game-style chirps",
  "version": 1,
  "defaults": {
    "wave": "square",
    "vel": 0.9,
    "env": { "a": 2, "d": 15, "s": 0.4, "r": 10 }
  },
  "sounds": {
    "button_tap": {
      "description": "Fast 3-note arpeggio click",
      "bpm": 180,
      "steps": [
        { "note": "C6", "dur": "16n", "vel": 0.95, "duty": 0.25,
          "env": { "a": 1, "d": 8, "s": 0, "r": 5 } },
        { "note": "E6", "dur": "16n", "vel": 0.9, "duty": 0.25,
          "env": { "a": 1, "d": 8, "s": 0, "r": 5 } },
        { "note": "G6", "dur": "16n", "vel": 0.85, "duty": 0.25,
          "env": { "a": 1, "d": 8, "s": 0, "r": 5 } }
      ]
    },
    "toggle_on": {
      "description": "Quick rising two-note chirp",
      "steps": [
        { "note": "C5", "dur": 25, "vel": 0.9, "duty": 0.25,
          "env": { "a": 1, "d": 10, "s": 0.3, "r": 8 } },

    "nav_back": {
      "description": "Classic game-style descending chirp",
      "bpm": 170,
      "steps": [
        { "note": "C6", "dur": "16n", "vel": 0.9, "duty": 0.5,
          "env": { "a": 1, "d": 8, "s": 0.2, "r": 5 } },
        { "note": "G5", "dur": "16n", "vel": 0.9, "duty": 0.5,
          "env": { "a": 1, "d": 8, "s": 0.2, "r": 5 } },
        { "note": "E5", "dur": "16n", "vel": 0.9, "duty": 0.5,
          "env": { "a": 1, "d": 10, "s": 0.3, "r": 8 } },
        { "note": "C5", "dur": "8n", "vel": 0.85, "duty": 0.5,
          "env": { "a": 1, "d": 15, "s": 0.2, "r": 12 } }
      ]
    },
    "dropdown_open": {
      "description": "Snappy 8-bit pop",
      "steps": [
        { "freq": 2200, "dur": 8, "vel": 0.85, "duty": 0.25,
          "env": { "a": 1, "d": 7, "s": 0, "r": 3 } },

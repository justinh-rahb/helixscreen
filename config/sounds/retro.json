          "env": { "a": 1, "d": 12, "s": 0.3, "r": 10 } }
      ]
    },
    "toggle_off": {
      "description": "Quick falling two-note chirp",
      "steps": [
        { "note": "G5", "dur": 25, "vel": 0.9, "duty": 0.25,
          "env": { "a": 1, "d": 10, "s": 0.3, "r": 8 } },
        { "note": "C5", "dur": 35, "vel": 0.85, "duty": 0.25,
          "env": { "a": 1, "d": 12, "s": 0.3, "r": 10 } }
      ]
    },
    "nav_forward": {
      "description": "Classic game-style ascending chirp",
      "bpm": 170,
      "steps": [
        { "note": "C5", "dur": "16n", "vel": 0.9, "duty": 0.5,
          "env": { "a": 1, "d": 8, "s": 0.2, "r": 5 } },
        { "note": "E5", "dur": "16n", "vel": 0.9, "duty": 0.5,
          "env": { "a": 1, "d": 8, "s": 0.2, "r": 5 } },

        { "note": "E5", "dur": "8n", "vel": 0.9, "duty": 0.5,
          "env": { "a": 2, "d": 15, "s": 0.6, "r": 10 } },
        { "note": "G5", "dur": "8n", "vel": 0.95, "duty": 0.5,
          "env": { "a": 2, "d": 15, "s": 0.6, "r": 10 } },
        { "pause": 30 },
        { "note": "A5", "dur": "8n", "vel": 0.95, "duty": 0.5,
          "env": { "a": 2, "d": 18, "s": 0.7, "r": 12 } },
        { "note": "B5", "dur": "8n", "vel": 0.95, "duty": 0.5,
          "env": { "a": 2, "d": 18, "s": 0.7, "r": 12 } },
        { "note": "C6", "dur": "4n.", "vel": 1.0, "duty": 0.5,
          "env": { "a": 2, "d": 60, "s": 0.8, "r": 80 } }
      ]
    },
    "print_cancelled": {
      "description": "Game over — descending minor arpeggio",
      "bpm": 160,
      "steps": [
        { "note": "E5", "dur": "8n", "vel": 0.9, "duty": 0.5,
          "env": { "a": 2, "d": 20, "s": 0.5, "r": 15 } },
        { "note": "C5", "dur": "8n", "vel": 0.85, "duty": 0.5,
          "env": { "a": 2, "d": 20, "s": 0.5, "r": 15 } },
        { "note": "A4", "dur": "8n", "vel": 0.8, "duty": 0.5,
          "env": { "a": 2, "d": 25, "s": 0.4, "r": 20 } },

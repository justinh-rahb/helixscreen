        { "note": "G4", "dur": "8n", "vel": 0.7, "duty": 0.5,
          "env": { "a": 2, "d": 30, "s": 0.3, "r": 25 } },
        { "note": "E4", "dur": "4n", "vel": 0.6, "duty": 0.5,
          "env": { "a": 3, "d": 50, "s": 0.2, "r": 60 } }
